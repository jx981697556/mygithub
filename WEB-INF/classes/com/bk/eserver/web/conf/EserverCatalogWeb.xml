<!DOCTYPE struts PUBLIC 
	"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
	"http://struts.apache.org/dtds/struts-2.0.dtd">
<!--将该脚本加入struts2配置文件中      资源服务分类<include file="com/bk/eserver/web/conf/EserverCatalogWeb.xml"></include> -->
<struts>
	<!-- 资源服务分类 -->
	<package name="EserverCatalog_Ajax_code" extends="json-default">
		<!-- 资源服务分类查询 -->
		<action name="EserverCatalogqueryAll" method="queryall"
			class="com.bk.eserver.web.action.ActionEserverCatalogQuery">
			<result type="json"></result>
		</action>
		<!-- 资源服务分类增加 -->
		<action name="EserverCatalogaddCommit" method="addSubmit"
			class="com.bk.eserver.web.action.ActionEserverCatalogQuery">
			<result type="json"></result>
		</action>
		<!-- 资源服务分类删除 -->
		<action name="EserverCatalogdeleteCommit" method="delSubmit"
			class="com.bk.eserver.web.action.ActionEserverCatalogQuery">
			<result type="json"></result>
		</action>
		<!-- 资源服务分类修改 -->
		<action name="EserverCatalogeditCommit" method="editSubmit"
			class="com.bk.eserver.web.action.ActionEserverCatalogQuery">
			<result type="json"></result>
		</action>
		<!-- 加载组织机构_树状列表 -->
		<action name="EserverCatalogLoadTreeNode" method="loadTreeNode"  
            class="com.bk.eserver.web.action.ActionEserverCatalogQuery">  
            <result type="json">  
                <param name="root">treeNodes</param>  
            </result>  
        </action>  
	</package>
	<package name="EserverCatalog_Jsp_code" extends="struts-default">
		<!-- 资源服务分类详细【修改、浏览】 -->
		<action name="EserverCatalogshow" method="view"
			class="com.bk.eserver.web.action.ActionEserverCatalogQuery">
			<result>/APP-ESERVER/ADMIN/pEserverCatalogEntity.jsp</result>
		</action>
		<!-- html加载资源服务分类控制台 -->
		<action name="EserverCatalog_ACTION_CONSOLE">
			<result>/APP-ESERVER/ADMIN/pEserverCatalogLayout.jsp</result>
		</action>
		<!-- ajax加载资源服务分类选择页面 -->
		<action name="EserverCatalog_ACTION_ALERT" >
			<result>/APP-ESERVER/ADMIN/pEserverCatalogGrid.jsp</result>
		</action>
	</package>
</struts>