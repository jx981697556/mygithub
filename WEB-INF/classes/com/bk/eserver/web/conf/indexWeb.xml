<!DOCTYPE struts PUBLIC 
	"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
	"http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="eServer_Ajax_code" extends="json-default">
		<action name="FP0004" class="com.bk.eserver.web.action.ActionSearchMap" method="info">
           <result type="json"></result>
        </action>
	</package>
	<package name="eServer_Jsp_code" extends="struts-default">
		<!-- e服务前台首页 -->
		<action name="FP0001" class="com.bk.eserver.web.action.ActionSearchMap" method="index">
			<result>/APP-ESERVER/index.jsp</result>
		</action>
		<!-- 执行模糊查询 -->
		<action name="FP0002" class="com.bk.eserver.web.action.ActionSearchMap" method="search">
			<result>/APP-ESERVER/search.jsp</result>
		</action>
		<!-- 执行分类查询 -->
		<action name="FP0003" class="com.bk.eserver.web.action.ActionSearchMap" method="typeSearch">
			<result>/APP-ESERVER/search.jsp</result>
		</action>
		<!-- 查看服务信息
		<action name="FP0004" class="com.bk.eserver.web.action.ActionSearchMap" method="info">
			<result>/APP-ESERVER/info.jsp</result>
		</action>-->
		<!-- 查询线路-->
		<action name="FP0005" class="com.bk.eserver.web.action.ActionSearchMap" method="directions">
			<result>/APP-ESERVER/directions.jsp</result>
		</action>
		<!-- 查询线路展示-->
		<action name="FP0006" class="com.bk.eserver.web.action.ActionSearchMap" method="directionsShow">
			<result name="success_t">/APP-ESERVER/directionsShowT.jsp</result>
			<result name="success_d">/APP-ESERVER/directionsShowD.jsp</result>
		</action>
	</package>
</struts>